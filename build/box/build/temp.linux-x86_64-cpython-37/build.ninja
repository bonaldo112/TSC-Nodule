ninja_required_version = 1.3
cxx = c++

cflags = -pthread -B /root/anaconda3/envs/nodule/compiler_compat -Wl,--sysroot=/ -Wsign-compare -DNDEBUG -g -fwrapv -O3 -Wall -Wstrict-prototypes -fPIC -I/root/anaconda3/envs/nodule/lib/python3.7/site-packages/torch/include -I/root/anaconda3/envs/nodule/lib/python3.7/site-packages/torch/include/torch/csrc/api/include -I/root/anaconda3/envs/nodule/lib/python3.7/site-packages/torch/include/TH -I/root/anaconda3/envs/nodule/lib/python3.7/site-packages/torch/include/THC -I/root/anaconda3/envs/nodule/include/python3.7m -c
post_cflags = -DTORCH_API_INCLUDE_EXTENSION_H '-DPYBIND11_COMPILER_TYPE="_gcc"' '-DPYBIND11_STDLIB="_libstdcpp"' '-DPYBIND11_BUILD_ABI="_cxxabi1011"' -DTORCH_EXTENSION_NAME=box -D_GLIBCXX_USE_CXX11_ABI=0 -std=c++14
ldflags = 

rule compile
  command = $cxx -MMD -MF $out.d $cflags -c $in -o $out $post_cflags
  depfile = $out.d
  deps = gcc



build /usr/local/nodule_det/NoduleNet/build/box/build/temp.linux-x86_64-cpython-37/box.o: compile /usr/local/nodule_det/NoduleNet/build/box/box.cpp





